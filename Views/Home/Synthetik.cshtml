@{
    ViewData["Title"] = ViewBag.Elementos[0];
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
</head>
<body>
    <h1 class="display-1 text-center">@ViewBag.Elementos[0]</h1>
    <div id="carouselExample" class="carousel slide">
        <div class="carousel-inner">
            <div class="carousel-item active">
                <iframe width="1100" height="615" src="@ViewBag.Elementos[1]" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" referrerpolicy="strict-origin-when-cross-origin" allowfullscreen></iframe>
            </div>
            <div class="carousel-item">
                <img src="/imgs/@ViewBag.Folder/ImgSynth1.jpg" class="d-block w-100" alt="...">
            </div>
            <div class="carousel-item">
                <img src="/imgs/@ViewBag.Folder/ImgSynth2.jpg" class="d-block w-100" alt="...">
            </div>
            <div class="carousel-item">
                <img src="/imgs/@ViewBag.Folder/ImgSynth3.jpg" class="d-block w-100" alt="...">
            </div>
        </div>
        <button class="carousel-control-prev" type="button" data-bs-target="#carouselExample" data-bs-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Previous</span>
        </button>
        <button class="carousel-control-next" type="button" data-bs-target="#carouselExample" data-bs-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Next</span>
        </button>
    </div>
    <h2 class="text-center">@ViewBag.Elementos[2]</h2>
    <p class="text-center container">@ViewBag.Elementos[3]</p>
    <img src="/imgs/@ViewBag.Folder/GifSynth1.gif" class="img-fluid mx-auto d-block" alt="...">
    <h2 class="text-center">@ViewBag.Elementos[4]</h2>
    <p class="text-center container">@ViewBag.Elementos[5]</p>
    <img src="/imgs/@ViewBag.Folder/GifSynth2.gif" class="img-fluid mx-auto d-block" alt="...">
    <h2 class="text-center">@ViewBag.Elementos[6]</h2>
    <p class="text-center container">@ViewBag.Elementos[7]</p>
    <img src="/imgs/@ViewBag.Folder/GifSynth3.gif" class="img-fluid mx-auto d-block" alt="...">
    <h2 class="text-center">@ViewBag.Elementos[8]</h2>
    <table class="table table-striped">
        <thead>
            <tr>
                <th scope="col">@ViewBag.Elementos[9]</th>
                <th scope="col">@ViewBag.Elementos[10]</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>@ViewBag.Elementos[11]</td>
                <td>@ViewBag.Elementos[12]</td>
            </tr>
            <tr>
                <td>@ViewBag.Elementos[13]</td>
                <td>@ViewBag.Elementos[14]</td>
            </tr>
            <tr>
                <td>@ViewBag.Elementos[15]</td>
                <td>@ViewBag.Elementos[16]</td>
            </tr>
            <tr>
                <td>@ViewBag.Elementos[17]</td>
                <td>@ViewBag.Elementos[18]</td>
            </tr>
            <tr>
                <td>@ViewBag.Elementos[19]</td>
                <td>@ViewBag.Elementos[20]</td>
            </tr>
            <tr>
                <td>@ViewBag.Elementos[21]</td>
                <td>@ViewBag.Elementos[22]</td>
            </tr>
            <tr>
                <td>@ViewBag.Elementos[23]</td>
                <td>@ViewBag.Elementos[24]</td>
            </tr>
            <tr>
                <td>@ViewBag.Elementos[25]</td>
                <td>@ViewBag.Elementos[26]</td>
            </tr>
            <tr>
                <td>@ViewBag.Elementos[27]</td>
                <td>@ViewBag.Elementos[28]</td>
            </tr>
            <tr>
                <td>@ViewBag.Elementos[29]</td>
                <td>@ViewBag.Elementos[30]</td>
            </tr>
        </tbody>
    </table>
    <img src="/imgs/@ViewBag.Folder/GifSynth4.gif" class="img-fluid mx-auto d-block" alt="...">
</body>
</html>