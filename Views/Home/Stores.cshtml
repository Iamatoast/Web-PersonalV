@{
    ViewData["Title"] = @ViewBag.Elementos[0];
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
</head>
<body>
    <h1 class="text-center">@ViewBag.Elementos[0]</h1>
    <p class="text-center">@ViewBag.Elementos[1]</p>
    <table class="table table-bordered">
        <thead>
            <tr>
                <th scope="col">@ViewBag.Elementos[2]</th>
                <th scope="col">@ViewBag.Elementos[3]</th>
                <th scope="col">@ViewBag.Elementos[4]</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <th scope="row">1</th>
                <td class="text-center">
                    @ViewBag.Elementos[5]
                    <img src="/imgs/@ViewBag.Folder/ImgStores1.png" alt="">
                </td>
                <td>
                    @ViewBag.Elementos[6]
                    <br> @ViewBag.Elementos[7]
                    <br> @ViewBag.Elementos[8]
                    <br> @ViewBag.Elementos[9]
                    <br> @ViewBag.Elementos[10]
                </td>
            </tr>
            <tr>
                <th scope="row">2</th>
                <td class="text-center">
                    @ViewBag.Elementos[11]
                    <img src="/imgs/@ViewBag.Folder/ImgStores2.png" alt="">
                </td>
                <td>
                    @ViewBag.Elementos[12]
                    <br> @ViewBag.Elementos[13]
                    <br> @ViewBag.Elementos[14]
                    <br> @ViewBag.Elementos[15]
                    <br> @ViewBag.Elementos[16]
                    <br> @ViewBag.Elementos[17]
                    <br> @ViewBag.Elementos[18]
                </td>
            </tr>
            <tr>
                <th scope="row">3</th>
                <td class="text-center">
                    @ViewBag.Elementos[19]
                    <img src="/imgs/@ViewBag.Folder/ImgStores3.png" alt="">
                </td>
                <td>
                    @ViewBag.Elementos[20]
                    <br> @ViewBag.Elementos[21]
                    <br> @ViewBag.Elementos[22]
                    <br> @ViewBag.Elementos[23]
                    <br> @ViewBag.Elementos[24]
                </td>
            </tr>
            <tr>
                <th scope="row">4</th>
                <td class="text-center">
                    @ViewBag.Elementos[25]
                    <img src="/imgs/@ViewBag.Folder/ImgStores4.png" alt="">
                </td>
                <td>
                    @ViewBag.Elementos[26]
                    <br> @ViewBag.Elementos[27]
                    <br> @ViewBag.Elementos[28]
                </td>
            </tr>
        </tbody>
    </table>
</body>
</html>